class Variables{
    void loss(){
        double initialamount;
        double finalamount;
        double loss;

        initialamount = 1000.0;
        system.out.println("Initial amount is: " + initialamount);
        loss = 300.0;
        finalamount = initialamount - loss;
        system.out.println("Your final amount is: " + finalamount);

    }
}